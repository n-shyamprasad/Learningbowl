@model IEnumerable<Emp.ModelLayer.Employee>
@{
    ViewBag.Title = "Employees";
}

<div class="jumbotron">
    <h2>Employee List</h2>
    <p class="lead">Employee list loaded from WebAPI</p>
</div>
<div class="jumbotron">
    <table class="table table-bordered">
        <thead>
            <tr>
                <th>Employee ID</th>
                <th>Employee Name</th>
                <th>Mobile Number</th>
                <th>Address</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var employee in Model)
            {
                <tr>
                    <th scope="row"> @employee.Id</th>
                    <td>@employee.EmpName</td>
                    <td>@employee.EmpMobile</td>
                    <td>@employee.EmpAddress</td>
                </tr>
            }
        </tbody>
    </table>
</div>


